{
  "name": "culinary-assessment",
  "version": "1.0.0",
  "description": "Transform your culinary leadership skills with our comprehensive assessment platform.",
  "license": "./LICENSE",
  "scripts": {
    "build": "next build",
    "clean": "rm -rf .next node_modules && npm cache clean --force && npm install",
    "clean:win": "powershell -ExecutionPolicy Bypass -File ./scripts/clean-dev.ps1",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit migrate",
    "db:reset": "docker-compose down -v && docker-compose up -d",
    "db:seed": "tsx ./db/seed.ts",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down -v",
    "db:studio": "npx drizzle-kit studio",
    "dev": "next dev --turbopack",
    "dev:clean": "npm run clean && npm run dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "node server.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@mantine/charts": "^7.17.5",
    "@mantine/core": "^7.17.4",
    "@mantine/dates": "^7.17.8",
    "@mantine/hooks": "^7.17.4",
    "@mantine/notifications": "^7.17.5",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tabler/icons-react": "^3.31.0",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.41.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "mailersend": "^2.6.0",
    "next": "^15.5.2",
    "next-safe-action": "^7.10.5",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "postgresql": "^0.0.1",
    "puppeteer": "^24.7.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.55.0",
    "recharts": "2",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/types": "^19.8.0",
    "@eslint/js": "^9.24.0",
    "@faker-js/faker": "^9.7.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/conventional-commits-parser": "^5.0.1",
    "@types/d3-array": "^3.2.2",
    "@types/d3-color": "^3.1.3",
    "@types/d3-ease": "^3.0.2",
    "@types/d3-interpolate": "^3.0.4",
    "@types/d3-path": "^3.1.1",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-shape": "^3.1.7",
    "@types/d3-time": "^3.0.4",
    "@types/pg": "^8.15.5",
    "@types/react": "19.1.0",
    "drizzle-kit": "^0.30.6",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "postcss": "^8.5.3",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "3.5.3",
    "prettier-plugin-packagejson": "^2.5.10",
    "tsx": "^4.19.3",
    "typescript-eslint": "^8.29.1"
  },
  "packageManager": "pnpm@10.8.0"
}
